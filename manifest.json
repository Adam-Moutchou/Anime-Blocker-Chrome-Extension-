{
  "manifest_version": 3,
  "name": "Anime Blocker",
  "description": "Block anime websites and Get rid of your anime addiction!",
  "version": "1.0",
  "action": {
    "default_icon": "Images/icon_180.png"
  },
  "content_scripts": [
    {
      "js": ["content_scripts/content.js"],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}